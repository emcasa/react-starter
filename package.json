{
  "name": "react-starter",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=12.10",
    "yarn": ">=1.16"
  },
  "scripts": {
    "start": "razzle start",
    "start:prod": "NODE_ENV=production node build/server.js",
    "build": "razzle build",
    "build:docker": "NODE_ENV=production razzle build && docker build .",
    "build:storybook": "build-storybook",
    "test": "razzle test --env=jsdom",
    "test:storyshots": "storyshots",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "storybook": "start-storybook -p 6006"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/razzle/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/?(*.)(spec|test).js"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "@absinthe/socket": "^0.2.1",
    "@absinthe/socket-apollo-link": "^0.2.1",
    "@apollo/react-ssr": "^3.1.1",
    "@emcasa/ui": "^2.4.7",
    "@emcasa/ui-dom": "^2.4.7",
    "@fortawesome/pro-solid-svg-icons": "^5.10.2",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-utilities": "^1.3.2",
    "connected-react-router": "^6.5.2",
    "cookie": "^0.4.0",
    "cross-fetch": "^3.0.4",
    "express": "^4.17.1",
    "graphql": "^14.5.5",
    "graphql-tag": "^2.10.1",
    "history": "^4.10.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "phoenix": "^1.4.10",
    "razzle": "^3.0.0",
    "react": "^16.9.0",
    "react-apollo": "^3.1.1",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.4",
    "redux-saga": "1.1.1",
    "reselect": "^4.0.0",
    "styled-components": "^4.3.2",
    "styled-system": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-react-constant-elements": "^7.6.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addon-storyshots": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "apollo-link-logger": "^1.2.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-react-require": "^3.1.1",
    "babel-plugin-styled-components": "^1.10.6",
    "chai": "^4.2.0",
    "chai-things": "^0.2.0",
    "enzyme": "^3.10.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "jsdom": "^15.1.1",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.9.0",
    "redux-logger": "^3.0.6",
    "redux-saga-test-factory": "^0.1.1",
    "require-context.macro": "^1.2.1",
    "webpack-blocks": "^2.0.1"
  }
}
