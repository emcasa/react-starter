{
  "displayName": "node",
  "testEnvironment": "node",
  "setupFiles": [
    "<rootDir>/test/setup/node.env.js"
  ],
  "setupFilesAfterEnv": [
    "<rootDir>/test/setup/global.setup.js"
  ],
  "testMatch": [
    "<rootDir>/test/src/**/?(*.)(spec|test).js",
    "<rootDir>/test/src/**/?(*.)node(spec|test).js"
  ]
}
