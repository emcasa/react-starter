{
  "name": "browser",
  "testEnvironment": "jsdom",
  "setupFiles": [
    "<rootDir>/test/setup/browser.env.js"
  ],
  "setupFilesAfterEnv": [
    "<rootDir>/test/setup/global.setup.js"
  ],
  "testMatch": [
    "<rootDir>/test/src/components/**/?(*.)(spec|test).js",
    "<rootDir>/test/src/redux/**/?(*.)(spec|test).js"
  ]
}
